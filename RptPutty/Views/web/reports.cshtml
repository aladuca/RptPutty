@model List<RptPutty.Models.Listing>

@{
    ViewBag.Title = "reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12">
    <h2>Reports</h2>
    @if (@Model.Count == 0)
    {<div class="alert alert-info" role="alert">No Epic Subscribed Reports.</div>}
    else
    {
        <ul class="list-unstyled">
            @foreach (var report in @Model)
            {
                <li><a href="~/web/generator/@report.Filename">@report.Title</a></li>
            }
        </ul>
    }
</div>
